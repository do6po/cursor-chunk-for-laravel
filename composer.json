{
    "name": "do6po/laravel-cursor-chunk",
    "description": "An easy way to get chunked data from database by cursor functional.",
    "type": "library",
    "version": "1.0.0",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/database": "^7|^8",
        "illuminate/support": "^7|^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8|^9",
        "orchestra/testbench": "^4|^5",
        "mockery/mockery": "^1.0",
        "laravel/framework": "^7|^8",
        "fakerphp/faker": "^1.9.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Boiko Sergii",
            "email": "helvisbox@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Do6po\\LaravelCursorChunk\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Do6po\\LaravelCursorChunk\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Do6po\\LaravelCursorChunk\\Providers\\CursorChunkServiceProvider"
            ]
        }
    }
}
